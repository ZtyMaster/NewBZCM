                    url: "/BZCM/FileUpload",
                    type: "post"
                })
                $("#DelImageBtn").show();
            });
            //删除上传图片
            $("#DelImageBtn").click(function () {
                var image = $("#DivShowImage").attr("src");
                $.post("/BZCM/DelImage", { str: image }, function (data) {
                    if (data.ret = "ok") {
                        $("#DivShowImage").remove();
                        $("#hiddenMenuIcon").val("");
                        $("#fileUp").val("");
                        $("#DelImageBtn").hide();
                        
                    }                    
                })
            })
            $("#AddFCKX").css("display", "none");
           
            $("#JingPinFangYuan").hide();
            $("#FangYuanXinXi").hide();
            $(".easyui-combobox").combobox("select", "2");
            //房产信息
            $("#Btnfcdata").click(function () {
                toggleKZ()
                $("#TTdiv").show();
