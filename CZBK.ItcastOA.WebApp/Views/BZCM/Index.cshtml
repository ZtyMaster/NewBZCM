        function afterAdd(data) {
            if (data.ret == "ok") {
                try {
                }
                catch (e) { }
                try {
                    $('#tt').datagrid('reload');
                    $('#AddBxBill').dialog('close');
                }
                catch (e) { }
                try {
                    $('#BillShow').datagrid('reload');
                }
                catch (e) {}
            } else  {
                $.messager.alert("提示", data.ret, "error");
            }
        }
        function formatCurrency(num) {
            num = num.toString().replace(/\$|\,/g, '');
            if (isNaN(num))
                num = "0";
            sign = (num == (num = Math.abs(num)));
            num = Math.floor(num * 100 + 0.50000000001);
            cents = num % 100;
            num = Math.floor(num / 100).toString();
            if (cents < 10)
                cents = "0" + cents;
            for (var i = 0; i < Math.floor((num.length - (1 + i)) / 3) ; i++)
                num = num.substring(0, num.length - (4 * i + 3)) + ',' +
                num.substring(num.length - (4 * i + 3));
            return (((sign) ? '' : '-') + num + '.' + cents);
        }
        function Addbtn() {
            clenrInpyt()
            $("#AddFCKX").css("display", "block");
            $("#JingPinFangYuan").show();
            $('#AddFCKX').dialog({
                title: "新增信息",
                width: 750,
                height: 400,
                collapsible: true,
                resizable: true,
                modal: true,
                buttons: [{
                    text: '确定',
                    iconCls: 'icon-ok',
                    handler: function () {
                        
                        $("#AddFCKXFrom").submit();//提交表单     
                        $('#AddFCKX').dialog('close');
                    }
                }, {
                    text: '取消',
                    handler: function () {
                        $('#AddFCKX').dialog('close');
                    }
                }]
            });
        }
        function changeScenes() {
            TabIsHide();
            var val = $("#FristItems").combobox("getValue");
            if (val == "2") {
                $("#JingPinFangYuan").show();
                $("#Imagebody").show();
            }
            else if (val == "5") {
                $("#JingPinFangYuan").show();
                $("#Imagebody").show();
            }
            else if (val == "6") {
                $("#NewKuaiXun").show();
                $("#Imagebody").show();
            }
            else if (val == "7") {
                $("#FangYuanXinXi").show();
            }
            else if (val == "4") {
                $("#JingPinFangYuan").show();
                $("#Imagebody").show();
            }
        }
        function BanrChangeScenes() {
            var val = $("#BtnInput").combobox("getValue");
            var pars = { "item": val }
            loadData(pars)
            
        }
        function TabIsHide() {
            $("#JingPinFangYuan").hide();
            $("#FangYuanXinXi").hide();
            $("#NewKuaiXun").hide();
            $("#Imagebody").hide();
            
            
        }
        //修改房源信息
        function eidtdata() {
            var rows = $('#tt').datagrid('getSelections');
            if (rows.length != 1) {
                //alert("请选择要修改的商品！");
                $.messager.alert("提醒", "请选择要编辑的1条记录!", "error");
                return;
            }
            Addbtn();
            $("#AddID").val(rows[0].ID);
            $("#IsTop").combobox("select", rows[0].IsTop)
            $("#FristItems").combobox("select", rows[0].itemsid)
            var startTime = CDFHoursMinute(rows[0].IsTopStartTime);
            var stopTime = CDFHoursMinute(rows[0].IsTopStopTime);
            $("#IsTopStartTime").datetimebox("setValue", startTime);
            $("#IsTopStopTime").datetimebox("setValue", stopTime);
            $("#IsTop_shor").val(rows[0].IsTop_shor);
            $("#Str_Name").val(rows[0].Str_Name);
            $("#hiddenMenuIcon").val(rows[0].Str_Image);
            $("#DivShowImage").remove();
            if (rows[0].Str_Image != null) {
                $("#DelImageBtn").show();
                $("#showImage").append("<img id='DivShowImage' src='" + rows[0].Str_Image + "' width='200' height='100'/>");
            }

            $("#FYXX_Name").val(rows[0].FYXX_Name);
            $("#FYXX_Photo").val(rows[0].FYXX_Photo);
            $("#FYXX_ONE").val(rows[0].FYXX_ONE);
            $("#FYXX_TWO").val(rows[0].FYXX_TWO);
            $("#FYXX_SHRER").val(rows[0].FYXX_SHRER);
            $("#News_Name").val(rows[0].News_Name);
            $("#News_Photo").val(rows[0].News_Photo);
            $("#News_KaiFaShang").val(rows[0].News_KaiFaShang);
            $("#News_Addess").val(rows[0].News_Addess);
            $("#News_Money").val(rows[0].News_Money);
            $("#News_Item").val(rows[0].News_Item);
            $("#News_YouHui").val(rows[0].News_YouHui);
            $("#News_Text").val(rows[0].News_Text);
        }
        //删除房源信息
        function deldata() {
            var rows = $('#tt').datagrid('getSelections');
            if (rows.length != 1) {
                //alert("请选择要修改的商品！");
                $.messager.alert("提醒", "请选择要编辑的1条记录!", "error");
                return;
            }

            messcon(rows[0].ID,"是否确认删除？");            
        }
        //撤销回收站
        function ReDel() {
            var rows = $('#tt').datagrid('getSelections');
            if (rows.length != 1) {
                //alert("请选择要修改的商品！");
                $.messager.alert("提醒", "请选择要编辑的1条记录!", "error");
                return;
            }

            messcon(rows[0].ID, "是否确认撤销？");
        }
        function messcon(val,str) {
            $.messager.confirm("提示", str, function (r) {
                if (r) {
                    $.post("/bzcm/deldata", { id: val }, function (dat) {
                        if (dat.ret == "ok") {
                            $("#tt").datagrid("reload");
                        } else {
                            $.messager.alert("提示", dat.ret, "err");
                        }
                    })
                }
            })
        }
        function clenrInpyt() {
            $("#AddID").val("");
            $("#IsTop").combobox("select", 0)
            $("#FristItems").combobox("select", 2)
            $("#IsTopStartTime").datetimebox("setValue", "");
            $("#IsTopStopTime").datetimebox("setValue", "");
            $("#IsTop_shor").val("");
            $("#Str_Name").val("");
            $("#hiddenMenuIcon").val("");
            $("#DivShowImage").remove();           
            $("#FYXX_Name").val("");
            $("#FYXX_Photo").val("");
            $("#FYXX_ONE").val("");
            $("#FYXX_TWO").val("");
            $("#FYXX_SHRER").val("");
            $("#News_Name").val("");
            $("#News_Photo").val("");
            $("#News_KaiFaShang").val("");
            $("#News_Addess").val("");
            $("#News_Money").val("");
            $("#News_Item").val("");
            $("#News_YouHui").val("");
            $("#News_Text").val("");
        }
        function toggleKZ() {
            $("#TTdiv").hide();
            $("#Wxdiv").hide();
            
        }
    </script>
