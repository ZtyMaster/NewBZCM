        function loadData(pars) {
            var Jpfy = true;
            var Fckx = true;
            var Fyxx = true;
            var NewKX = true;
            var imagebol = true;
            
            if (pars != undefined) {
                switch (pars.item) {
                    case "2": Jpfy = false, imagebol = false;
                        break;
                    case "7": Fyxx = false;
                        break;
                    case "5": Jpfy = false, imagebol = false;
                        break;
                    case "6": NewKX = false, imagebol = false;
                        break;
                    case "4": Jpfy = false, imagebol = false;
                        break;
                }
                pars = { "item": pars.item, "IsHSZ": IsHSZ }
            }
           
            $('#tt').datagrid({
                url: '/BZCM/GetFanChan',
