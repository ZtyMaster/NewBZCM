                    { field: 'Str_Name', title: '企业名称', align: 'center', hidden: Jpfy,editor:'text' },
                    { field: 'AddUser', title: '创建人员', align: 'center' },
                    { field: 'IsTop_shor', title: '排序', align: 'center' },
                    { field: 'FYXX_Name', title: '房源信息名称', align: 'center', hidden: Fyxx, align: 'center' },
                     { field: 'FYXX_Photo', title: '房源信息电话', hidden: Fyxx, align: 'center' },
                    { field: 'FYXX_ONE', title: '房源信息第一行', hidden: Fyxx, align: 'center' },
                    { field: 'FYXX_TWO', title: '房源信息第三行', hidden: Fyxx, align: 'center' },
                    { field: 'FYXX_SHRER', title: '房源信息第二行', hidden: Fyxx, align: 'center' },
                    { field: 'News_Addess', title: '地址', align: 'center', hidden: NewKX },
                    { field: 'News_Name', title: '项目名称', align: 'center', hidden: NewKX },
                    { field: 'News_Item', title: '房屋类型', align: 'center', hidden: NewKX },
                    { field: 'News_KaiFaShang', title: '开发商', align: 'center', hidden: NewKX },
                    { field: 'News_Money', title: '房屋价位', align: 'center', hidden: NewKX },
                    { field: 'News_Photo', title: '销售电话', align: 'center', hidden: NewKX },
