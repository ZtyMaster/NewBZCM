                pagination: true,//是否有分页
                singleSelect: true,//是否单行选择
                pageSize: 25,//页大小，一页多少条数据
                pageNumber: 1,//当前页，默认的
                pageList: [15, 25, 35],
                queryParams: pars,//往后台传递参数
                columns: [[//c.UserName, c.UserPass, c.Email, c.RegTime
                    {
                        field: 'edit', title: '操作', align: 'center', hidden: IsHSZ, formatter: function (value, row) {
                            return "<a href='javascript:eidtdata()' class='l-btn' style='padding:2px 5px;'>修改</a>";
                        }
